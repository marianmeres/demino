{
  "name": "@marianmeres/demino",
  "version": "1.3.2",
  "exports": "./src/mod.ts",
  "tasks": {
    "dev": "deno run --watch ./src/mod.ts",
    "test": "deno test -A",
    "test:watch": "deno test -A --watch",
    "release": "deno run -A jsr:@marianmeres/deno-release",
    "rp": "deno task release -y && deno publish",
    "rpm": "deno task release minor -y && deno publish"
  },
  "imports": {
    "@marianmeres/clog": "jsr:@marianmeres/clog@^3.8.2",
    "@marianmeres/http-utils": "jsr:@marianmeres/http-utils@^2.0.2",
    "@marianmeres/midware": "jsr:@marianmeres/midware@^1.3.0",
    "@marianmeres/simple-router": "jsr:@marianmeres/simple-router@^3.1.0",
    "@std/assert": "jsr:@std/assert@^1.0.16",
    "@std/encoding": "jsr:@std/encoding@^1.0.10",
    "@std/fmt": "jsr:@std/fmt@^1.0.8",
    "@std/fs": "jsr:@std/fs@^1.0.20",
    "@std/http": "jsr:@std/http@^1.0.22",
    "@std/path": "jsr:@std/path@^1.1.3",
    "request-ip": "npm:request-ip@^3.3.0"
  },
  "publish": {
    "exclude": [
      ".*",
      "tests/**"
    ]
  },
  "fmt": {
    "useTabs": true,
    "lineWidth": 90,
    "indentWidth": 4
  }
}
